{
  "name": "zoonotify-client",
  "version": "0.0.1",
  "description": "Web-basierte Abfrage von Zoonosen Surveillancedaten",
  "scripts": {
    "lint": "./node_modules/.bin/eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "./node_modules/.bin/eslint . --fix --ext .js,.jsx,.ts,.tsx",
    "serve": "npx nodemon -e ts,tsx --exec \"npm run serve:pipe\"",
    "build": "./node_modules/.bin/webpack && npm run cp:all",
    "prebuild": "npm run clean",
    "clean": "npx rimraf public/*",
    "serve:pipe": "npm run build && npm run start",
    "start": "./node_modules/.bin/http-server -P http://localhost:3000 -c-1 public",
    "cp:html": "npx copyfiles -u 1 ./src/**/*.html ./public",
    "cp:assets": "npx copyfiles -u 1 ./src/assets/* ./public",
    "cp:localesDe": "npx copyfiles -u 1 -E ./src/locales/de/* ./public",
    "cp:localesEn": "npx copyfiles -u 1 -E ./src/locales/en/* ./public",
    "cp:all": "npm run cp:html && npm run cp:assets && npm run cp:localesDe && npm run cp:localesEn",
    "test": "./node_modules/.bin/jest",
    "test:coverage": "./node_modules/.bin/jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://review.gerrithub.io/SiLeBAT/zoonotify-client"
  },
  "keywords": [],
  "author": "Michele Kayser, Dominic TÃ¶lle",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "./hooks/commit-msg ./.git/COMMIT_EDITMSG"
    }
  },
  "lint-staged": {
    "*.ts|*.tsx": [
      "npm run lint"
    ]
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@types/lodash": "^4.14.159",
    "clsx": "^1.1.1",
    "i18next": "^19.5.1",
    "i18next-browser-languagedetector": "^6.0.1",
    "i18next-xhr-backend": "^3.2.2",
    "lodash": "^4.17.20",
    "moment": "^2.27.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-download-link": "^2.3.0",
    "react-i18next": "^11.7.0",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
