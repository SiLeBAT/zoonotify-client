{
  "name": "zoonotify-client",
  "version": "3.0.0",
  "description": "Web-basierte Abfrage von Zoonosen Surveillancedaten",
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --fix --ext .js,.jsx,.ts,.tsx",
    "start": "cross-env npm run clean && npm run cp:all && webpack-dev-server --open",
    "start:qa": "cross-env npm run clean && npm run cp:all && webpack-dev-server --node-env qa --open",
    "start:prod": "cross-env npm run clean && npm run cp:all && webpack-dev-server --node-env production --open",
    "build": "cross-env webpack",
    "build:qa": "cross-env webpack --node-env qa",
    "build:prod": "cross-env webpack --node-env production",
    "prebuild": "npm run clean",
    "clean": "npx rimraf public/*",
    "cp:html": "npx copyfiles -u 1 ./src/**/*.html ./public",
    "cp:assets": "npx copyfiles -u 1 ./src/assets/* ./public",
    "cp:localesDe": "npx copyfiles -u 1 -E ./src/locales/de/* ./public",
    "cp:localesEn": "npx copyfiles -u 1 -E ./src/locales/en/* ./public",
    "cp:all": "npm run cp:html && npm run cp:assets && npm run cp:localesDe && npm run cp:localesEn",
    "test": "jest",
    "test:coverage": "./node_modules/.bin/jest --coverage",
    "format": "npx prettier --write src/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SiLeBAT/zoonotify-client.git"
  },
  "keywords": [],
