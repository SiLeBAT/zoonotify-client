{
  "name": "zoonotify-client",
  "version": "0.0.1",
  "description": "Web-basierte Abfrage von Zoonosen Surveillancedaten",
  "scripts": {
    "prepare": "husky install",
    "lint": "./node_modules/.bin/eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "./node_modules/.bin/eslint . --fix --ext .js,.jsx,.ts,.tsx",
    "serve": "npm run clean && npm run cp:all && webpack-dev-server --open",
    "build": "./node_modules/.bin/webpack && npm run cp:all",
    "prebuild": "npm run clean",
    "clean": "npx rimraf public/*",
    "cp:html": "npx copyfiles -u 1 ./src/**/*.html ./public",
    "cp:assets": "npx copyfiles -u 1 ./src/assets/* ./public",
    "cp:localesDe": "npx copyfiles -u 1 -E ./src/locales/de/* ./public",
    "cp:localesEn": "npx copyfiles -u 1 -E ./src/locales/en/* ./public",
    "cp:all": "npm run cp:html && npm run cp:assets && npm run cp:localesDe && npm run cp:localesEn",
    "test": "./node_modules/.bin/jest",
    "test:coverage": "./node_modules/.bin/jest --coverage",
    "format": "npx prettier --write src/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SiLeBAT/zoonotify-client.git"
  },
  "keywords": [],
  "author": "Michele Kayser, Dominic TÃ¶lle",
  "license": "MIT",
