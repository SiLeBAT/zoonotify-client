    "i18next-http-backend": "^1.3.1",
    "jszip": "^3.7.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.2",
    "react": "^17.0.2",
    "react-apexcharts": "^1.3.9",
    "react-csv": "^2.0.3",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.14.2",
    "react-router-dom": "^5.2.0"
