  "scripts": {
    "lint": "./node_modules/.bin/tslint -c tslint.json 'src/**/*.tsx'",
    "serve": "npx nodemon -e ts,tsx --exec \"npm run serve:pipe\"",
    "build": "./node_modules/.bin/webpack && npm run cp:all",
    "prebuild": "npm run clean",
    "clean": "npx rimraf public/*",
    "serve:pipe": "npm run build && ./node_modules/.bin/http-server -P http://localhost:3000 public",
    "cp:html": "npx copyfiles -u 1 ./src/**/*.html ./public",
    "cp:assets": "npx copyfiles -u 1 ./src/assets/* ./public",
    "cp:all": "npm run cp:html && npm run cp:assets",
    "test": "./node_modules/.bin/jest",
    "test:coverage": "./node_modules/.bin/jest --coverage"
  },
