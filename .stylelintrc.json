{
  "extends": "stylelint-config-standard",
  "rules": {
    "selector-type-no-unknown": [
      true,
      {
        "ignoreTypes": [
          "router-outlet"
        ]
      }
    ],
    "at-rule-no-unknown": [
      true,
      {
        "ignoreAtRules": [
          "mixin",
          "include"
        ]
      }
    ]
  }
}